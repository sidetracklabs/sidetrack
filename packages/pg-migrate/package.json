{
  "name": "@sidetrack/pg-migrate",
  "version": "0.0.8",
  "description": "Sidetrack internal postgres database migration tool",
  "keywords": [
    "background",
    "jobs",
    "queue",
    "worker",
    "postgresql",
    "postgres",
    "tasks",
    "typescript",
    "effect",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidetracklabs/sidetrack",
    "directory": "packages/pg-migrate"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --config ./vitest.config.ts --coverage",
    "migrate": "dotenv sidetrack-pg-migrate up",
    "test": "dotenv -- vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "mkdirp": "~3.0.1"
  },
  "devDependencies": {
    "@types/mkdirp": "2.0.0",
    "@types/pg": "8.10.2",
    "@vitest/coverage-v8": "0.34.3",
    "dotenv-cli": "7.3.0",
    "pg": "8.11.3",
    "type-fest": "4.3.1",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.3"
  },
  "peerDependencies": {
    "pg": ">=4.3.0 <9.0.0"
  }
}
