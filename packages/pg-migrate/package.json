{
  "name": "@sidetrack/pg-migrate",
  "version": "0.1.2",
  "description": "Sidetrack internal postgres database migration tool",
  "keywords": [
    "background",
    "jobs",
    "queue",
    "worker",
    "postgresql",
    "postgres",
    "tasks",
    "typescript",
    "effect",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidetracklabs/sidetrack",
    "directory": "packages/pg-migrate"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --config ./vitest.config.ts --coverage",
    "migrate": "dotenv sidetrack-pg-migrate up",
    "test": "dotenv -- vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "mkdirp": "~3.0.1"
  },
  "devDependencies": {
    "@types/pg": "8.11.11",
    "@vitest/coverage-v8": "3.0.8",
    "dotenv-cli": "8.0.0",
    "pg": "8.13.3",
    "vite": "6.2.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8"
  },
  "peerDependencies": {
    "pg": ">=4.3.0 <9.0.0"
  },
  "sideEffects": false
}
