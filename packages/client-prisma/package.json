{
  "name": "@sidetrack/client-prisma",
  "version": "0.0.18",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "background",
    "jobs",
    "queue",
    "worker",
    "postgresql",
    "postgres",
    "tasks",
    "typescript",
    "effect",
    "database",
    "prisma"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidetracklabs/sidetrack",
    "directory": "packages/client-prisma"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --config ./vitest.config.ts --coverage",
    "postinstall-steps": "prisma generate --schema=./test/schema.prisma",
    "test": "dotenv -- vitest",
    "test-ci": "dotenv -- vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@prisma/client": "6.3.0",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "2.1.8",
    "dotenv-cli": "7.4.4",
    "prisma": "6.3.0",
    "sidetrack": "workspace:*",
    "vite": "6.0.7",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "peerDependencies": {
    "@prisma/client": ">= 4"
  }
}
