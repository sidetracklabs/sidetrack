{
  "name": "@sidetrack/adapter-prisma",
  "version": "0.0.1",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "background",
    "jobs",
    "queue",
    "worker",
    "postgresql",
    "postgres",
    "tasks",
    "typescript",
    "effect",
    "database",
    "prisma"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidetracklabs/sidetrack",
    "directory": "packages/adapter-prisma"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --config ./vitest.config.ts --coverage",
    "test": "pnpm run prisma-client-gen && dotenv vitest --config ./vitest.config.ts",
    "typecheck": "tsc --noEmit",
    "prisma-client-gen": "prisma generate --schema=./test/schema.prisma"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@prisma/client": "5.1.0",
    "@sidetrack/sidetrack": "workspace:*",
    "@types/uuid": "9.0.2",
    "@vitest/coverage-v8": "0.34.0",
    "dotenv-cli": "7.2.1",
    "prisma": "5.1.0",
    "vite": "4.4.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.0"
  },
  "peerDependencies": {
    "@prisma/client": ">= 4"
  }
}
