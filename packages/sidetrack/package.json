{
  "name": "@sidetrack/sidetrack",
  "version": "0.0.4",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "background",
    "jobs",
    "queue",
    "worker",
    "postgresql",
    "postgres",
    "tasks",
    "typescript",
    "effect",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidetracklabs/sidetrack",
    "directory": "packages/sidetrack"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./effect": {
      "types": "./dist/effect.d.ts",
      "import": "./dist/effect.mjs",
      "require": "./dist/effect.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --config ./vitest.config.ts --coverage",
    "generate-db-models": "dotenv kanel",
    "migrate": "dotenv sidetrack-pg-migrate up",
    "test": "dotenv vitest --config ./vitest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@effect/data": "^0.17.0",
    "@effect/io": "^0.37.0",
    "@sidetrack/pg-migrate": "^0.0.1",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "@types/pg": "8.10.2",
    "@vitest/coverage-v8": "0.34.0",
    "dotenv-cli": "7.2.1",
    "kanel": "3.4.3",
    "type-fest": "4.1.0",
    "vite": "4.4.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.0"
  }
}
