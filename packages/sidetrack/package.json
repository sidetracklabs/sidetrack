{
  "name": "sidetrack",
  "version": "0.1.4",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "background",
    "jobs",
    "queue",
    "worker",
    "postgresql",
    "postgres",
    "tasks",
    "typescript",
    "effect",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidetracklabs/sidetrack",
    "directory": "packages/sidetrack"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./effect": {
      "import": {
        "types": "./dist/effect.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/effect.d.ts",
        "default": "./dist/effect.js"
      }
    }
  },
  "bin": {
    "sidetrack": "./dist/cli.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --config ./vitest.config.ts --coverage",
    "generate-db-models": "dotenv kanel",
    "migrate": "dotenv sidetrack-pg-migrate up",
    "test": "dotenv -- vitest",
    "test-ci": "dotenv -- vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@effect/cli": "^0.56.1",
    "@effect/platform-node": "^0.73.1",
    "@sidetrack/pg-migrate": "workspace:*",
    "effect": "3.13.1",
    "pg": "^8.13.3"
  },
  "devDependencies": {
    "@types/pg": "8.11.11",
    "@vitest/coverage-v8": "3.0.5",
    "dotenv-cli": "8.0.0",
    "kanel": "3.11.1",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5"
  }
}
